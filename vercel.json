{
  "buildCommand": "npm run build",
  "outputDirectory": ".output/public",
  "framework": "nuxt",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOWALL"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://web.telegram.org https://t.me"
        }
      ]
    }
  ]
}

